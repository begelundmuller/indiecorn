{
  "extends": ["next/core-web-vitals", "prettier", "plugin:import/recommended"],
  "plugins": ["import"],
  "rules": {
    "react/prop-types": 0,
    "react/no-unescaped-entities": 0,
    "import/order": [
      "error",
      {
        "groups": ["builtin", "external", ["internal", "unknown"], "parent", "sibling", "index"],
        "newlines-between": "always"
      }
    ]
  }
}
